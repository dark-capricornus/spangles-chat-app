{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],  "routes": [
    {
      "src": "/api/debug",
      "dest": "/api/debug.js"
    },
    {
      "src": "/api/simple/(.*)",
      "dest": "/api/simple.js"
    },    {
      "src": "/api/(.*)",
      "dest": "/api/minimal.js"
    },    {
      "src": "/uploads/(.*)",
      "dest": "/api/minimal.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/client/build/manifest.json"
    },
    {
      "src": "/favicon.ico",
      "dest": "/client/build/favicon.ico"
    },
    {
      "src": "/static/(.*)",
      "dest": "/client/build/static/$1"
    },    {
      "src": "/(.*)",
      "dest": "/client/build/index.html"
    }
  ]
}
